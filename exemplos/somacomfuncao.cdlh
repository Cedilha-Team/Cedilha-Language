Funcao Inteiro somarPara (Inteiro max);

BlocoPrincipal {
	Inteiro resultado;
	
	
	
	resultado = #somarPara(10);
	Imprime(resultado);
}

BlocoFuncoes {

	Funcao Inteiro somarPara (Inteiro max) retorna  {
		Inteiro resultado;
		para(Inteiro i de 0 a max){
			resultado = resultado+i;
		}
		retorna resultado;
	}
	
}