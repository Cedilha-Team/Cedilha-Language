BlocoPrincipal{
    Real x, y, resultado;
    Inteiro c;
    
    Imprima("Digite o numero Real X:");
    Leia(x);
    
    Imprima("Digite o numero Real Y:");
    Leia(y);
    
    Imprima("Digite o numero Inteiro c:");
    Leia(c);
    
    resultado = c-y+ (x*x);
    
    Imprima("O resultado da operacao eh: ", resultado);
    
}